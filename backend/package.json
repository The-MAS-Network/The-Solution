{
  "name": "the-solution0backend",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "build": "tsc -p .",
    "dev": "nodemon ./src/main.ts",
    "start": "node ./dist/main.js",
    "typeorm": "npm run build && npx typeorm -d dist/db/data-source.js",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migrate": "npm run migration:generate -- ./src/db/migrations/migrationName && npm run migration:run",
    "create:admin:dev": "ts-node ./src/seeders/admin",
    "create:admin:prod": "node ./dist/seeders/admin",
    "migrate:prod": "npm run migration:generate -- ./src/db/prod-migrations/migrationName",
    "migration:run:prod": "npx typeorm -d dist/db/prod-data-source.js migration:run"
  },
  "author": "Ubong Jacob for the mas network competition",
  "license": "ISC",
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@stenway/reliabletxt": "^1.1.0",
    "apisauce": "^3.0.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.2",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "int64-buffer": "^1.0.1",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "lightning": "^10.6.1",
    "mysql2": "^3.6.5",
    "node-cron": "^3.0.3",
    "reflect-metadata": "^0.2.1",
    "resend": "^3.2.0",
    "typeorm": "^0.3.17",
    "typeorm-cursor-pagination": "^0.10.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
